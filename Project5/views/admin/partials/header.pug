header.header 
    .inner-logo 
        a(href=`/${pathAdmin}/dashboard`)
            span 28
            span Admin
    .inner-wrap     
        .inner-notify 
            img(src="/admin/assets/images/icon.svg" alt="notify")
            span 6
        .inner-account 
            .inner-avatar 
                img(src=account.avatar alt="avatar")
            .inner-content
                .inner-name #{account.fullname}
                .inner-role #{account.roleName}
            button.inner-button-menu
                img(src="/admin/assets/images/Menu.png" alt="arrow")
